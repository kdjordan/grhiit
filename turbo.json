{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".nuxt/**", ".output/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": [".output/**", "dist/**"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "preview": {
      "dependsOn": ["build"]
    }
  }
}